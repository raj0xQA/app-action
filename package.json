{
  "name": "qa-app-actiom",
  "version": "1.0.0",
  "description": "Chatway mobile app automation",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "emulator30:headless": "emulator -avd Pixel_API_30_ARM -no-snapshot -no-boot-anim -noaudio -no-window",
    "emulator30": "emulator -avd Pixel_API_30_ARM -no-snapshot -no-boot-anim -noaudio",
    "emulator34": "emulator -avd Pixel_API_34_ARM -no-snapshot -no-boot-anim -noaudio",
    "report:generate": "npx allure generate allure-results",
    "report:open": "npx allure open",
    "report:serve": "npx allure serve allure-results",
    "test:report": "npm run test && npm run report:serve",
    "test": "npx wdio run wdio.conf.js",
    "mail:action-fail": "node ./utils/send-action-fail-mail.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "clean": "rm -rf allure-results allure-report appium-logs screenshots screencasts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chatway-app/qa-chatway-mobile.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chatway-app/qa-chatway-mobile/issues"
  },
  "homepage": "https://github.com/Chatway-app/qa-chatway-mobile#readme",
  "devDependencies": {
    "@wdio/appium-service": "^9.16.2",
    "@wdio/cli": "^9.16.2",
    "@wdio/local-runner": "^9.16.2",
    "@wdio/mocha-framework": "^9.16.2",
    "@wdio/spec-reporter": "^9.16.2",
    "appium": "^2.19.0",
    "appium-uiautomator2-driver": "^4.2.4",
    "chromedriver": "^138.0.5",
    "dotenv": "^17.2.4",
    "webdriverio": "^9.16.2"
  },
  "dependencies": {
    "@wdio/allure-reporter": "^9.24.0",
    "nodemailer": "^8.0.1",
    "npm-check-updates": "^19.4.0"
  }
}
